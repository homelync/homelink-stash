{
  "name": "homelink-stash",
  "version": "1.0.0",
  "description": "Consumer for messages",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "npm": ">=7.21.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "nvm:default": ". ~/.nvm/nvm.sh && nvm alias default 16.18.0",
    "lint": "tslint -c \"./tslint.json\" -p \"./tsconfig.json\"",
    "docker:runprod": "docker run -p 3000:3000 -e NODE_ENV=production homelinksink",
    "docker:runlocal": "docker run -p 3000:3000 -e NODE_ENV=local homelinksink",
    "docker:rundev": "docker run -p 3000:3000 -e NODE_ENV=development homelinksink",
    "docker:runtest": "docker run -p 3000:3000 -e NODE_ENV=test homelinksink",
    "docker:build": "docker build . -t homelinksink",
    "test:unit": "mocha --exit --require ts-node/register src/test/unit/**/*.ts",
    "test:e2e": "cross-env TZ=UTC mocha --exit --timeout 30000 --require ts-node/register src/test/e2e/**/*.spec.ts",
    "test:e2e:debug": "cross-env TZ=UTC mocha --inspect-brk --exit --timeout 30000 --require ts-node/register src/test/e2e/**/*.spec.ts",
    "test:integration": "mocha --exit --timeout 30000 --require ts-node/register src/test/integration/**/*.spec.ts",
    "test:integration:debug": "mocha --inspect-brk --exit --timeout 30000 --require ts-node/register src/test/integration/**/*.spec.ts",
    "test:ci": "cross-env NODE_ENV=development npm run test:integration && cross-env NODE_ENV=development npm run test:e2e",
    "build": "npm --version && tsc --version && npm run clean && tsc && npm run copy:env",
    "start": "cross-env TZ=UTC NODE_ENV=local ts-node-dev --respawn ./src",
    "start:local": "cross-env TZ=UTC NODE_ENV=local ts-node-dev --respawn ./src",
    "start:dev": "cross-env TZ=UTC NODE_ENV=development ts-node-dev --respawn ./src",
    "start:test": "cross-env TZ=UTC NODE_ENV=test ts-node-dev --respawn ./src",
    "start:prod": "cross-env TZ=UTC NODE_ENV=production ts-node-dev --respawn ./src",
    "copy:env": "mkdir ./dist/env && cp -r -f ./src/env/.env* ./dist/env",
    "clean": "rm -rf ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.24",
    "@types/node-fetch": "^2.1.6",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "randomstring": "^1.1.5",
    "rimraf": "^2.6.3",
    "short-uuid": "^4.2.0",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.12.1",
    "typescript": "^4.1.5",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.4.1",
    "@types/amqp-connection-manager": "^2.0.10",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.8.0",
    "async-mqtt": "^2.6.2",
    "clone-deep": "^4.0.1",
    "cls-hooked": "https://github.com/Recodify/cls-hooked/tarball/master",
    "dotenv": "^7.0.0",
    "inversify": "^5.0.1",
    "knex": "^0.21.17",
    "moment-timezone": "^0.5.33",
    "mqtt": "^4.3.7",
    "mssql": "^6.4.1",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "npm": "^7.5.2",
    "reflect-metadata": "^0.1.3",
    "tedious": "^15.1.2",
    "winston": "^3.2.1",
    "winston-cloudwatch": "https://github.com/Recodify/winston-cloudwatch/tarball/master"
  }
}
